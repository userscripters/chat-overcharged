{
  "name": "chat-overcharge",
  "version": "1.5.0",
  "description": "Chat experience improvements",
  "scripts": {
    "test": "mocha",
    "lint": "eslint src --fix --ext .js,.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run lint && npm run clean",
    "postbuild": "npm run headers-tm",
    "uglify": "uglifyjs dist/index.js --compress --mangle -o dist/index.min.js",
    "uglify-es5": "uglifyjs dist/index.js --compress --mangle -o dist/index-es5.min.js",
    "build": "tsc && npm run uglify",
    "build-es5": "tsc --target es5 && npm run uglify-es5 && npm run postbuild",
    "build-all": "npm run prebuild && npm run build && npm run build-es5 && npm run postbuild",
    "headers-tm": "generate tampermonkey --output dist/headers.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userscripters/template.git"
  },
  "keywords": [
    "chat",
    "stackoverflow",
    "userscript"
  ],
  "author": "Oleg Valter",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/userscripters/template/issues"
  },
  "homepage": "https://github.com/userscripters/template#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@userscripters/generate-headers": "^1.0.6",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4",
    "uglify-js": "^3.13.7"
  }
}
